<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xm.commoncomponent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.TextViewAct" />
        <activity android:name=".ui.EditTextAct" />
        <activity android:name=".ui.ButtonAct" />
        <activity android:name=".ui.ImageViewAct" />
        <activity android:name=".ui.SeekBarAct" />
        <activity android:name=".ui.ScrollViewAct" />
