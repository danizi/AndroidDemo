<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xm.commoncomponent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextViewAct" />
        <activity android:name=".EditTextAct" />
        <activity android:name=".ButtonAct" />
        <activity android:name=".ImageViewAct" />
        <activity android:name=".SeekBarAct" />
        <activity android:name=".ScrollViewAct" />
        <activity android:name=".ViewPagerAct" />
        <activity android:name=".NotificationAct" />
        <activity android:name=".AlertDialogAct" />
        <activity android:name=".DrawerLayoutAct"></activity>
    </application>

</manifest>