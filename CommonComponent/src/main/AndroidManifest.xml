        <activity android:name=".ui.AlertDialogAct" />
        <activity android:name=".ui.DrawerLayoutAct" />
        <activity android:name=".ui.RadioButtonAct" />
        <activity android:name=".ui.ToggleButtonAct" />
        <activity android:name=".ui.ProgressBarAct" />
        <activity android:name=".ui.ListViewAct" />
        <activity android:name=".ui.GridViewAct" />
        <activity android:name=".ui.SpinnerAct" />
        <activity android:name=".ui.AutoCompleteTextViewAct" />
        <activity android:name=".ui.ViewFlipperAct" />
        <activity android:name=".ui.ToastAct" />
        <activity android:name=".ui.PopupWindowAct" />
        <activity android:name=".ui.MenuAct"></activity>
    </application>

</manifest>